        <register>
          <name>SPI_MEM_CTRL2</name>
          <description>SPI0 control2 register.</description>
          <addressOffset>0x10</addressOffset>
          <size>0x20</size>
          <resetValue>0x00002C21</resetValue>
          <fields>
            <field>
              <name>SPI_MEM_CS_SETUP_TIME</name>
